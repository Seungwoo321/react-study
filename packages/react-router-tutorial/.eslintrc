{
    "parser": "@babel/eslint-parser",
    "extends": ["standard", "standard-jsx", "standard-react"],
    "rules": {
        "indent": ["error", 4],
        "no-tabs": 0,
        "react/jsx-indent": [2, 4, {"checkAttributes": true, "indentLogicalExpressions": true}],
        "react/jsx-indent-props": [2, 4],
        "jsx-quotes": ["error", "prefer-double"],
        "react/prop-types": [0]
    }
}